<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    android:orientation="vertical" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/ren_pin_layout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingLeft="8dp"
                android:paddingRight="8dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <LinearLayout
                        android:id="@+id/dynamic_state_layoutnew"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true" >

                        <ImageView
                            android:layout_width="35dp"
                            android:layout_height="49dp"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/news" />

                        <TextView
                            android:id="@+id/renpin_textiview6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="@android:color/white"
                            android:textSize="15dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/zan"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/solid_grey"
                        android:orientation="horizontal"
                        android:padding="1px" >

                        <Button
                            android:id="@+id/renpin_button1"
                            android:layout_width="wrap_content"
                            android:layout_height="35dp"
                            android:background="#fbbc40"
                            android:paddingLeft="15dp"
                            android:paddingRight="15dp"
                            android:text="@string/near_text"
                            android:textColor="#ffffff"
                            android:textSize="15sp" />

                        <Button
                            android:id="@+id/renpin_button2"
                            android:layout_width="wrap_content"
                            android:layout_height="35dp"
                            android:background="#f0f0f0"
                            android:paddingLeft="15dp"
                            android:paddingRight="15dp"
                            android:text="@string/square"
                            android:textColor="@color/grey"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/renpin_announceimage"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:paddingRight="10dp"
                        android:src="@drawable/add" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ren_pin_linearlayout2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="#F5F5F5"
                android:orientation="vertical"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:visibility="gone" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <ImageView
                        android:id="@+id/customer_login_image1"
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/left" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_centerVertical="true"
                        android:text="动态"
                        android:textColor="#000000"
                        android:textSize="19sp" />
                </RelativeLayout>
            </LinearLayout>

            <!-- 更多顶部界面 -->

            <LinearLayout
                android:id="@+id/top_linearlayout2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="#F5F5F5"
                android:orientation="vertical"
                android:paddingLeft="8dp"
                android:paddingRight="8dp" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <ImageView
                        android:id="@+id/renpinmain_morereturnimageview"
                        android:layout_width="22dp"
                        android:layout_height="22dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/left" />

                    <TextView
                        android:id="@+id/customer_login_textview1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="@string/more_down_text"
                        android:textColor="@color/d_grey"
                        android:textSize="19sp" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#1e90ff"
                android:orientation="horizontal" >
            </LinearLayout>

            <LinearLayout
                android:id="@+id/upshowlayout_nonetwork"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="#87CEFA"
                android:orientation="horizontal"
                android:visibility="gone" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:paddingLeft="20dp" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="38dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/warnong" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="8dp" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/no_linkservice"
                        android:textColor="#000000" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                
                <com.renpin.renpincustomer.PullToRefreshView
                    android:id="@+id/main_pull_refresh_view"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:visibility="gone" >

                    <GridView
                        android:id="@+id/main_gridview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:horizontalSpacing="1.5dp"
                        android:numColumns="2"
                        android:stretchMode="columnWidth"
                        android:verticalSpacing="1.5dp" />

                    <TextView
                        android:id="@id/android:empty"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="#f5f5f5"
                        android:gravity="center"
                        android:text="@string/noMoreRecord"
                        android:textSize="20dp" />
                </com.renpin.renpincustomer.PullToRefreshView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="@drawable/frame_grey"
                android:orientation="horizontal"
                android:visibility="gone" >

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:paddingLeft="20dp" >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="36dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/location1" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="8dp"
                    android:paddingRight="5dp" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:text="adress"
                        android:textColor="@color/d_grey"
                        android:textSize="12dp" />
                </RelativeLayout>
            </LinearLayout>

            <!-- 我的显示界面 -->

            <ListView
                android:id="@+id/ren_pin_listView2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="#FFFFFF"
                android:cacheColorHint="#00000000"
                android:divider="@null"
                android:visibility="gone" >
            </ListView>

            <!-- 更多界面 -->

            <LinearLayout
                android:id="@+id/ren_pin_linearlayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:visibility="gone" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#F5F5F5"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/activity_renpin_myinfolinearlayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/frame_grey"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="20dp"
                            android:layout_marginTop="20dp"
                            android:orientation="horizontal" >

                            <ImageView
                                android:id="@+id/more_opera_imageview1"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:layout_marginLeft="10dp"
                                android:scaleType="centerCrop"
                                android:src="@drawable/noperson" />

                            <ImageView
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_gravity="top" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical" >

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal" >

                                    <TextView
                                        android:id="@+id/more_opera_Accounttextview1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:textColor="@color/blue"
                                        android:textSize="16dp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="8dp"
                                    android:background="@drawable/solid_grey0"
                                    android:orientation="horizontal"
                                    android:paddingBottom="3dp"
                                    android:paddingLeft="8dp"
                                    android:paddingRight="8dp"
                                    android:paddingTop="3dp" >

                                    <TextView
                                        android:id="@+id/more_opera_credittextview2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginLeft="8dp"
                                        android:text="人品:13"
                                        android:textColor="#FA8072"
                                        android:textSize="15dp" />

                                    <ImageView
                                        android:id="@+id/more_opera_imageview2"
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:layout_marginLeft="15dp"
                                        android:src="@drawable/zan1" />

                                    <TextView
                                        android:id="@+id/more_opera_charmtextview3"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center"
                                        android:layout_marginLeft="5dp"
                                        android:text="13"
                                        android:textColor="#FA8072"
                                        android:textSize="15dp" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <LinearLayout
                            android:id="@+id/upshowlayout3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical" >

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="0.1dp"
                                android:layout_marginTop="20dp"
                                android:background="@color/l_lightgrey"
                                android:orientation="horizontal" >
                            </View>

                            <RelativeLayout
                                android:id="@+id/renpinmain_relativelayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="#ffffff"
                                android:padding="15dp" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_centerVertical="true"
                                    android:text="@string/update"
                                    android:textColor="@color/d_grey"
                                    android:textSize="15dp" />

                                <TextView
                                    android:id="@+id/renpinmain_prompttextview"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerVertical="true"
                                    android:textColor="#FF0000" />
                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0.1dp"
                                android:background="@color/l_lightgrey"
                                android:orientation="horizontal" >
                            </LinearLayout>

                            <RelativeLayout
                                android:id="@+id/renpinmain_advice"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="#ffffff"
                                android:padding="15dp" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_centerVertical="true"
                                    android:text="@string/advice"
                                    android:textColor="@color/d_grey"
                                    android:textSize="15dp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerVertical="true" />
                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0.1dp"
                                android:background="@color/l_lightgrey"
                                android:orientation="horizontal" >
                            </LinearLayout>

                            <RelativeLayout
                                android:id="@+id/renpinmain_aboutus"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="#ffffff"
                                android:padding="15dp" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentLeft="true"
                                    android:layout_centerVertical="true"
                                    android:text="@string/aboutus"
                                    android:textColor="@color/d_grey"
                                    android:textSize="15dp" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_centerVertical="true" />
                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0.1dp"
                                android:background="@color/l_lightgrey"
                                android:orientation="horizontal" >
                            </LinearLayout>
                        </LinearLayout>
                    </ScrollView>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="bottom"
                android:orientation="horizontal"
                android:visibility="gone" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#292421"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:id="@+id/dynamic_state_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:background="#292421"
                        android:gravity="center" >

                        <TextView
                            android:id="@+id/renpin_textiview2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/dynamic_state_text"
                            android:textColor="@android:color/white"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/renpin_textiview6old"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textColor="@android:color/white"
                            android:textSize="15dp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/near_linear_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#292421"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:id="@+id/near_relative_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/renpin_textiview3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/near_state_text"
                            android:textColor="#ffffff"
                            android:textSize="15dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/share_linear_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#292421"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:id="@+id/space_state_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/renpin_textiview4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/space_state_text"
                            android:textColor="#ffffff"
                            android:textSize="15dp" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/more_linearlayout"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#292421"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:id="@+id/more_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" >

                        <TextView
                            android:id="@+id/renpin_textiview5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/more_state_text"
                            android:textColor="#ffffff"
                            android:textSize="15dp" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/renpinmain_moreoperalinear"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="50dp"
            android:background="#333333"
            android:orientation="vertical"
            android:padding="6dp" >

            <LinearLayout
                android:id="@+id/renpinmain_personinfolinearlayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="6dp"
                android:paddingTop="6dp" >

                <ImageView
                    android:id="@+id/renpinmain_personimage"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/news" />

                <TextView
                    android:id="@+id/renpinmain_personname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ellipsize="end"
                    android:paddingLeft="5dp"
                    android:singleLine="true"
                    android:text="name"
                    android:textColor="#ffffff"
                    android:textSize="15dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/renpinmain_logintextview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:padding="8dp"
                android:text="@string/land_name"
                android:textColor="#ffffff"
                android:textSize="15dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_marginBottom="6dp"
                android:background="#1e90ff"
                android:orientation="horizontal" >
            </LinearLayout>

            <TextView
                android:id="@+id/renpinmain_annoucetextview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/makepublic_w"
                android:gravity="center"
                android:padding="8dp"
                android:text="@string/make_public"
                android:textColor="#ffffff"
                android:textSize="15dp" />

            <TextView
                android:id="@+id/renpinmain_moretextview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/more"
                android:gravity="center"
                android:padding="8dp"
                android:text="@string/more_down_text"
                android:textColor="#ffffff"
                android:textSize="15dp" />
        </LinearLayout>
    </FrameLayout>

</LinearLayout>